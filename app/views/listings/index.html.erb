<h1 class="hero-title text-center">food listings</h1>
<section class="container">
    <div class="row-cols-sm-1 row-cols-md-2 row-cols-lg-4">
        <%@listings.each do |listing|%>
        <div class="col mb-4"></div>
            <div class="card border-dark h-100">
                <%=image_tag"foodtubs.jpg", class: "thumbnail card-img top", alt: "A few tubs of cooked food"%>
                <div class="card-body">
                    <h5><%=listing.title%></h5>
                    <p class="card-text text-secondary"><%=listing.description%></p>
                </div>
            </div>
        </div>
        <%end%>
    </div>

</section>
